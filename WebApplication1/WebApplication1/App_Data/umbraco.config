      <projectItem id="1086" key="946aea2d-9c4c-4856-95a9-8a80b104ac22" parentID="1085" level="3" creatorID="0" sortOrder="0" createDate="2019-02-11T13:14:35" updateDate="2019-03-19T11:42:08" nodeName="TJ Maxx" urlName="tj-maxx" path="-1,1076,1085,1086" isDoc="" nodeType="1074" creatorName="Kenneth Jegindfeldt" writerName="Kenneth Jegindfeldt" writerID="0" template="1073" nodeTypeAlias="projectItem" isPublished="true">
