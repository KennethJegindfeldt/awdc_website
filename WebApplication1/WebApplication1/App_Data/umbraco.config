      </projectItem>
      <projectItem id="1101" key="4fa3068e-d8ab-4906-ae49-3e3f9bbc2371" parentID="1085" level="3" creatorID="0" sortOrder="4" createDate="2019-02-19T10:04:28" updateDate="2019-03-19T11:58:15" nodeName="Lowe's" urlName="lowes" path="-1,1076,1085,1101" isDoc="" nodeType="1074" creatorName="Kenneth Jegindfeldt" writerName="Kenneth Jegindfeldt" writerID="0" template="1073" nodeTypeAlias="projectItem" isPublished="true">
